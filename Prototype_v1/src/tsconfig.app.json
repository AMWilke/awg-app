{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "outDir": "../out-tsc/app",
        "baseUrl": "./",
        "paths": {
            "@awg-app/*": ["app/*"],
            "@awg-core/*": ["app/core/*"],
            "@awg-shared/*": ["app/shared/*"],
            "@awg-side-info/*": ["app/side-info/*"],
            "@awg-views/*": ["app/views/*"]
        },
        "module": "es2015",
        "types": []
    },
    "include": ["**/*.ts"],
    "exclude": ["test.ts", "**/*.spec.ts", "testing"]
}
